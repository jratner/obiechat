<div class="userAccountContainer">
  {{#if user}}
    <div class="userInfo">
      <div class="displayName">
        {{ displayName }}
      </div>
      <div class="anonName">
        {{ anonName }}
      </div>
    </div>
    <div class="userActions">
    </div>
  {{ else }}
    <a class="signinLink" href="/auth/google" data-pushstate=true data-bypass=true>Sign In With Google</a>
    <div>test</div>
  {{/if}}
</div>